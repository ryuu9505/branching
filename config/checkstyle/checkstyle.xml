<?xml version="1.0"?>
<!DOCTYPE module PUBLIC
        "-//PUBLISHER//DTD Checkstyle Configuration 1.3//EN"
        "https://checkstyle.sourceforge.io/dtds/configuration_1_3.dtd">
<module name="Checker">
    <module name="TreeWalker">
        <module name="WhitespaceAround"/>
        <module name="AvoidInlineConditionals"/>
        <module name="EmptyBlock"/>
        <module name="IllegalCatch"/>
        <module name="JavadocMethod"/>
    </module>
    <module name="Header">
        <property name="header" value="/* Generated by Checkstyle */"/>
    </module>
    <module name="RegexpHeader">
        <property name="header" value="^/* This file is part of .*"/>
    </module>
</module>